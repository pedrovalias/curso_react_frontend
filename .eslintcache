    loadTasks();\n  }, [filterActived]) // pode ser passado mais parametros ou nenhum, depende da necessidade\n\n  return (\n    <S.Container>\n\n      <Header />\n\n      <S.FilterArea>\n\n        {/* Utilizando os cards como botões para possibilitar o reconhecimento do clique\n            passando o parametro desejado */}\n        <button type=\"button\" onClick={() => setFilterActived(\"all\")}>\n          <FilterCard title=\"Todos\" actived={filterActived === 'all'}/>\n        </button>\n\n        <button type=\"button\" onClick={() => setFilterActived(\"today\")}>\n          <FilterCard title=\"Hoje\" actived={filterActived === 'today'}/>\n        </button>\n\n        <button type=\"button\" onClick={() => setFilterActived(\"week\")}>\n          <FilterCard title=\"Semana\" actived={filterActived === 'week'}/>\n        </button>\n\n        <button type=\"button\" onClick={() => setFilterActived(\"month\")}>\n          <FilterCard title=\"Mês\" actived={filterActived === 'month'}/>\n        </button>\n\n        <button type=\"button\" onClick={() => setFilterActived(\"year\")}>\n          <FilterCard title=\"Ano\" actived={filterActived === 'year'}/>\n        </button>\n\n      </S.FilterArea>\n\n      <S.Title>\n        <h3>Tarefas</h3>\n      </S.Title>\n      \n      <S.Content>\n        {/* O map percorre item a item dentro da coleção - é uma estrutura de repetiçao */}\n        {\n          tasks.map(t => (\n            <TaskCard />\n          ))\n        }\n      </S.Content>\n\n      <Footer />\n\n    </S.Container>\n  )\n}\n\nexport default Home;\n","/home/pedrorv/Documentos/Meus Projetos/curso_node-react-mongodb/Frontend/web/src/components/Header/index.js",["64","65","66","67"],"import React from 'react';\nimport * as S from './styles';\n\n// Importação de imagens\nimport logo from '../../assets/logo.png';\nimport bell from '../../assets/bell.png';\n\nfunction Header() {\n  return (\n    <S.Container>\n\n      <S.LeftSide>\n        <img src={logo} alt=\"Logo\" />\n      </S.LeftSide>\n\n      <S.RightSide>\n        <a href=\"#\">INÍCIO</a>\n        <span className=\"divider\"/>\n        <a href=\"#\">NOVA TAREFA</a>\n        <span className=\"divider\"/>\n        <a href=\"#\">SINCRONIZAR CELULAR</a>\n        <span className=\"divider\"/>\n        <a href=\"#\" id=\"notification\">\n          <img src={bell} alt=\"Notificação\"/>\n          <span>5</span>\n        </a>\n      </S.RightSide>\n\n    </S.Container>\n  )\n}\n\nexport default Header;\n","/home/pedrorv/Documentos/Meus Projetos/curso_node-react-mongodb/Frontend/web/src/views/Home/styles.js",[],"/home/pedrorv/Documentos/Meus Projetos/curso_node-react-mongodb/Frontend/web/src/components/Header/styles.js",[],"/home/pedrorv/Documentos/Meus Projetos/curso_node-react-mongodb/Frontend/web/src/components/Footer/index.js",[],"/home/pedrorv/Documentos/Meus Projetos/curso_node-react-mongodb/Frontend/web/src/components/Footer/styles.js",[],"/home/pedrorv/Documentos/Meus Projetos/curso_node-react-mongodb/Frontend/web/src/components/FilterCard/index.js",[],"/home/pedrorv/Documentos/Meus Projetos/curso_node-react-mongodb/Frontend/web/src/components/FilterCard/styles.js",[],"/home/pedrorv/Documentos/Meus Projetos/curso_node-react-mongodb/Frontend/web/src/components/TaskCard/index.js",[],"/home/pedrorv/Documentos/Meus Projetos/curso_node-react-mongodb/Frontend/web/src/components/TaskCard/styles.js",[],["68","69"],"/home/pedrorv/Documentos/Meus Projetos/curso_node-react-mongodb/Frontend/web/src/services/api.js",[],{"ruleId":"70","replacedBy":"71"},{"ruleId":"72","replacedBy":"73"},{"ruleId":"70","replacedBy":"74"},{"ruleId":"72","replacedBy":"75"},{"ruleId":"76","severity":1,"message":"77","line":42,"column":6,"nodeType":"78","endLine":42,"endColumn":21,"suggestions":"79"},{"ruleId":"80","severity":1,"message":"81","line":17,"column":9,"nodeType":"82","endLine":17,"endColumn":21},{"ruleId":"80","severity":1,"message":"81","line":19,"column":9,"nodeType":"82","endLine":19,"